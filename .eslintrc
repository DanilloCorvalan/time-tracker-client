{
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "space-before-function-paren": 0,
    "func-names": 0,
    "semi": 0,
    "new-cap": 0,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 2,
    "no-param-reassign": 0
  },
  "plugins": [
    "react"
  ]
}
